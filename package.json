{
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-scss": "^4.0.9",
    "tailwindcss": "^3.4.3"
  },
  "scripts": {
    "build:tailwind": "postcss ./src/css/style.css -o ./src/output.css",
    "watch:tailwind": "postcss ./src/css/style.css -o ./src/output.css --watch",
    "watch": "npm-run-all -p watch:tailwind start"
  },
  "dependencies": {
    "flowbite": "^2.3.0"
  }
}
